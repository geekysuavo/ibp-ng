{* phi *}
assign (resid  1 and name C)  (resid  2 and name N)
       (resid  2 and name CA) (resid  2 and name C) 1.0 -60.0 10.0 1
assign (resid  2 and name C)  (resid  3 and name N)
       (resid  3 and name CA) (resid  3 and name C) 1.0 -60.0 10.0 1
assign (resid  3 and name C)  (resid  4 and name N)
       (resid  4 and name CA) (resid  4 and name C) 1.0 -60.0 10.0 1
assign (resid  4 and name C)  (resid  5 and name N)
       (resid  5 and name CA) (resid  5 and name C) 1.0 -60.0 10.0 1
assign (resid  5 and name C)  (resid  6 and name N)
       (resid  6 and name CA) (resid  6 and name C) 1.0 -60.0 10.0 1
assign (resid  6 and name C)  (resid  7 and name N)
       (resid  7 and name CA) (resid  7 and name C) 1.0 -60.0 10.0 1
assign (resid  7 and name C)  (resid  8 and name N)
       (resid  8 and name CA) (resid  8 and name C) 1.0 -60.0 10.0 1
assign (resid  8 and name C)  (resid  9 and name N)
       (resid  9 and name CA) (resid  9 and name C) 1.0 -60.0 10.0 1
assign (resid  9 and name C)  (resid 10 and name N)
       (resid 10 and name CA) (resid 10 and name C) 1.0 -60.0 10.0 1

